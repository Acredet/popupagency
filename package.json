{
	"name": "popupagency",
	"version": "1.0.0",
	"private": true,
	"scripts": {
		"commit": "git add . && git commit -m ",
		"addDist": "cd .nuxt && sudo mkdir dist && cd dist && sudo mkdir client && cd client && sudo mkdir img",
		"dev": "cross-env NODE_ENV=development nodemon server/index.js --watch server",
		"generate": "nuxt generate",
		"push": "git push origin master",
		"start": "node server/index.js",
		"stop": "taskkill -f -im node.exe",
		"build": "nuxt build",
		"export": "nuxt export",
		"serve": "nuxt serve",
		"format": "prettier -write **/**/.[js,vue]"
	},
	"dependencies": {
		"@braid/vue-formulate": "^2.4.5",
		"@nuxtjs/auth": "^4.9.1",
		"@nuxtjs/axios": "^5.12.2",
		"@nuxtjs/dotenv": "^1.4.1",
		"bcrypt": "^5.0.0",
		"bcryptjs": "^2.4.3",
		"bootstrap": "^4.5.3",
		"bootstrap-vue": "^2.18.0",
		"cors": "^2.8.5",
		"cross-env": "^7.0.2",
		"dotenv": "^8.2.0",
		"express": "^4.17.1",
		"hooper": "^0.3.4",
		"jimp": "^0.16.1",
		"joi": "^14.3.1",
		"jsonwebtoken": "^8.5.1",
		"lodash": "^4.17.20",
		"mongoose": "^5.10.9",
		"multer": "^1.4.2",
		"node-geocoder": "^3.27.0",
		"nodemailer": "^6.4.14",
		"nodemon": "^2.0.6",
		"nuxt": "^2.14.7",
		"nuxt-i18n": "^6.15.4",
		"v-viewer": "^1.5.1",
		"vue-slider-component": "^3.2.7",
		"vue2-editor": "^2.10.2",
		"vue2-google-maps": "^0.10.7"
	},
	"devDependencies": {
		"@nuxt/types": "^2.14.7",
		"prettier": "^2.1.2",
		"pretty-quick": "^3.1.0",
		"webpack-watch-files-plugin": "^1.1.0",
		"yorkie": "^2.0.0"
	},
	"gitHooks": {
		"pre-commit": "pretty-quick --staged"
	}
}
